
one = sum(cumsum(seq(from = 2, by = 8, length.out = 500)) + 1) #lowerright 

two = sum(cumsum(seq(from = 8, by = 8, length.out = 500)) + 1) #upperright 

three = sum(cumsum(seq(from = 6, by = 8, length.out = 500)) + 1) #upperright

four = sum(cumsum(seq(from = 4, by = 8, length.out = 500)) + 1) #lowerleft
 
one + two + three + four + 1 #ANSWER 
